<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/partner_sift_loseweight__webview"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.boohee.boohee.View.Partner_View.Sift_loseweight_WebView_Activity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_marginLeft="60dp">

            <TextView
                android:id="@+id/hot_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="bohe"
                android:textSize="20sp"
                android:textColor="#000000"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                />


        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/message_classification_Allread"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_alignParentRight="true"
            android:onClick="onclick">

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="15sp"
                android:textColor="#000000"
                android:layout_marginRight="10dp"
                android:layout_centerVertical="true"
                />


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/hot_webview_back"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:onClick="onclick"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true">

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:background="@drawable/as3a"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"/>

        </RelativeLayout>


    </RelativeLayout>

    <WebView
        android:id="@+id/hot_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>


</LinearLayout>